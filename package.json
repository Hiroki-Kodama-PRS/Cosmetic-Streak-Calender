{
  "name": "cosmetic-usage-tracker",
  "version": "1.0.0",
  "description": "化粧品使用量管理アプリ - 美容効果を最大化するスマートトラッキング",
  "main": "App.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "concurrently \"npm run backend:dev\" \"npm run mobile:start\"",
    "backend:dev": "cd backend && npm run dev",
    "mobile:start": "expo start",
    "db:migrate": "cd backend && npm run migrate",
    "db:seed": "cd backend && npm run seed",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "expo build"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-navigation": "^6.0.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "react-native-ble-manager": "^10.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-camera": "^4.2.1",
    "react-native-image-picker": "^5.0.0",
    "axios": "^1.5.0",
    "@react-native-async-storage/async-storage": "1.19.0",
    "react-native-paper": "^5.10.0",
    "react-native-elements": "^3.4.3",
    "expo-notifications": "~0.20.0",
    "expo-camera": "~13.4.0",
    "expo-media-library": "~15.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "typescript": "^5.1.3",
    "jest": "^29.2.1",
    "@testing-library/react-native": "^12.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "concurrently": "^8.0.0"
  },
  "keywords": [
    "cosmetics",
    "beauty",
    "skincare",
    "iot",
    "usage-tracking",
    "react-native",
    "mobile-app"
  ],
  "author": "Dariush & Hiroki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dariush-hiroki/cosmetic-usage-tracker.git"
  }
}